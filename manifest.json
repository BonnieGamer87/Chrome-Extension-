{
  "manifest_version": 3,
  "name": "Website Menu",
  "version": "0.1",
  "description": "A simple Chrome extension to display a menu of websites.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "https://i.ibb.co/Xs6Wn1b/16x16icon.png",
      "48": "https://i.ibb.co/1XH4MjH/48x48icon.png",
      "128": "https://i.ibb.co/McJrYzZ/128x128icon.png"
    }
  },
  "permissions": ["activeTab"]
}
